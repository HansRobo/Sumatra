<?xml version="1.0" encoding="UTF-8"?>
<botmgr base="edu.dhbw.mannheim.tigers.sumatra.model.modules.impls.botmanager">
    <bots>
        <TigerBot>
            <defaultKickerMaxCap class="int" comment="Default maximum Capacitor charge for kicker">150</defaultKickerMaxCap>
            <center2DribblerDist class="float" comment="Dist [mm] - Distance between center of bot to dribbling bar">75.0</center2DribblerDist>
        </TigerBot>
        <GrSimBot>
            <positionErrorMultiplier class="float" comment="multiplied on position error for PositionController">0.0042</positionErrorMultiplier>
            <defaultKickerMaxCap class="int" comment="Default maximum Capacitor charge for kicker">180</defaultKickerMaxCap>
            <useTrajectory class="boolean" comment="">true</useTrajectory>
            <center2DribblerDist class="float" comment="Dist [mm] - Distance between center of bot to dribbling bar">90.0</center2DribblerDist>
            <delay class="float" comment="">0.02</delay>
        </GrSimBot>
        <TigerBotV3>
            <feedbackFreqIdle class="int" comment="Desired frequency [Hz] of feedback commands from bot if idle">20</feedbackFreqIdle>
            <feedbackFreqActive class="int" comment="Desired frequency [Hz] of feedback commands from bot if active">80</feedbackFreqActive>
            <botNames class="[Ljava.lang.String;" comment="Bot names, ordered by id">Gandalf;Achilles;Eichbaum;Tigger;Optimus Prime;Odysseus;Leopard 3;Q;Reality Checkpoint;This Bot;Yoda;Hercules</botNames>
            <disableControllers class="boolean" comment="Disable controller on reset">true</disableControllers>
            <center2DribblerDist class="float" comment="Dist [mm] - Distance between center of bot to dribbling bar">85.0</center2DribblerDist>
        </TigerBotV3>
        <Performance>
            <accMax:GRSIM class="float" comment="">2</accMax:GRSIM>
            <brkMax:GRSIM class="float" comment="">2</brkMax:GRSIM>
            <velMax:GRSIM class="float" comment="">2.0</velMax:GRSIM>
            <accMaxW:GRSIM class="float" comment="">10</accMaxW:GRSIM>
            <brkMaxW:GRSIM class="float" comment="">10</brkMaxW:GRSIM>
            <velMaxW:GRSIM class="float" comment="">10</velMaxW:GRSIM>
        </Performance>
        <SimBot>
            <pidXYp class="float" comment="">0.005</pidXYp>
            <pidXYd class="float" comment="">0.0</pidXYd>
            <pidXYi class="float" comment="">0.0</pidXYi>
            <pidWp class="float" comment="">4.0</pidWp>
            <pidWd class="float" comment="">0.0</pidWd>
            <pidWi class="float" comment="">0.0</pidWi>
        </SimBot>
    </bots>
    <basestation>
        <BaseStation>
            <localPort:PRIMARY class="int" comment="">10200</localPort:PRIMARY>
            <localPort:SECONDARY class="int" comment="">10200</localPort:SECONDARY>
            <host:PRIMARY class="java.lang.String" comment="">10.42.0.254</host:PRIMARY>
            <host:SECONDARY class="java.lang.String" comment="">192.168.14.210</host:SECONDARY>
            <dstPort:PRIMARY class="int" comment="">10201</dstPort:PRIMARY>
            <dstPort:SECONDARY class="int" comment="">10201</dstPort:SECONDARY>
            <channel:PRIMARY class="int" comment="">121</channel:PRIMARY>
            <channel:SECONDARY class="int" comment="">110</channel:SECONDARY>
            <fixedRuntime:PRIMARY class="boolean" comment="Fix the runtime regardless of the number of bot that are connected.">true</fixedRuntime:PRIMARY>
            <fixedRuntime:SECONDARY class="boolean" comment="Fix the runtime regardless of the number of bot that are connected.">true</fixedRuntime:SECONDARY>
            <maxBots:PRIMARY class="int" comment="Max communication slots to open for communication to bots">9</maxBots:PRIMARY>
            <maxBots:SECONDARY class="int" comment="Max communication slots to open for communication to bots">8</maxBots:SECONDARY>
            <timeout:PRIMARY class="int" comment="timeout when bot is considered to be offline.">1000</timeout:PRIMARY>
            <timeout:SECONDARY class="int" comment="timeout when bot is considered to be offline.">1000</timeout:SECONDARY>
            <speed:PRIMARY
                class="edu.dhbw.mannheim.tigers.sumatra.model.modules.impls.botmanager.commands.basestation.BaseStationConfigV2$EWifiSpeed" comment="wifi speed (must be consistent with bots wifi speed!)">WIFI_SPEED_2M</speed:PRIMARY>
            <speed:SECONDARY
                class="edu.dhbw.mannheim.tigers.sumatra.model.modules.impls.botmanager.commands.basestation.BaseStationConfigV2$EWifiSpeed" comment="wifi speed (must be consistent with bots wifi speed!)">WIFI_SPEED_2M</speed:SECONDARY>
            <active:PRIMARY class="boolean" comment="If true, connect automatically on startup">true</active:PRIMARY>
            <active:SECONDARY class="boolean" comment="If true, connect automatically on startup">false</active:SECONDARY>
        </BaseStation>
    </basestation>
</botmgr>
