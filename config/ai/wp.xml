<?xml version="1.0" encoding="UTF-8"?>
<wp base="edu.dhbw.mannheim.tigers.sumatra.model.modules.impls.worldpredictor">
    <neural>
        <neuralimpl>
            <NeuralNetworkImpl>
                <numberofTrainingIterations class="int" comment="">1</numberofTrainingIterations>
                <lookaheadMS class="long" comment="">50</lookaheadMS>
            </NeuralNetworkImpl>
            <NeuralBallState>
                <throwOutTimeMS class="long" comment="">1000</throwOutTimeMS>
            </NeuralBallState>
            <NeuralRobotState>
                <throwOutTimeMS class="long" comment="">1000</throwOutTimeMS>
                <sslDefaultTeamSize class="int" comment="this value is always multiplyed by 2 to allow maximum coverage">6</sslDefaultTeamSize>
                <hashtableLoadFactor class="float" comment="">1.0</hashtableLoadFactor>
            </NeuralRobotState>
        </neuralimpl>
        <NeuralWP>
            <allowTrainingBlue class="boolean" comment="">true</allowTrainingBlue>
            <allowTrainigYellow class="boolean" comment="">true</allowTrainigYellow>
            <allowTrainingBall class="boolean" comment="">true</allowTrainingBall>
            <allowLoadingFromFile class="boolean" comment="allows loading the networks from the filesystem if present">true</allowLoadingFromFile>
        </NeuralWP>
    </neural>
    <WPFacade>
        <currentPredictors
            class="[Ledu.dhbw.mannheim.tigers.sumatra.model.modules.types.AWorldPredictor$PredictorKey;" comment="Allows switching/cumulating the Predictors">[Kalman,BOT]</currentPredictors>
        <fps class="int" comment="Rate at which the frames are published to the AI">60</fps>
    </WPFacade>
    <SyncedCamFrameBufferV2>
        <camToQuadrantAssociation:LAB
            class="[Ledu.dhbw.mannheim.tigers.sumatra.model.modules.impls.worldpredictor.SyncedCamFrameBufferV2$MathematicalQuadrants;" comment="possible keys: Q1,Q2,Q3,Q4,Q12,Q13,Q14,Q23,Q24,Q34,Q1234">[Q1234]</camToQuadrantAssociation:LAB>
        <camToQuadrantAssociation:SIM
            class="[Ledu.dhbw.mannheim.tigers.sumatra.model.modules.impls.worldpredictor.SyncedCamFrameBufferV2$MathematicalQuadrants;" comment="possible keys: Q1,Q2,Q3,Q4,Q12,Q13,Q14,Q23,Q24,Q34,Q1234">[Q1234]</camToQuadrantAssociation:SIM>
        <intersectionEpsilon:LAB class="double" comment="">50</intersectionEpsilon:LAB>
        <intersectionEpsilon:SIM class="double" comment="">0.0</intersectionEpsilon:SIM>
        <exclusionRectP1
            class="edu.dhbw.mannheim.tigers.sumatra.model.data.shapes.vector.IVector2" comment="P1 of rectangle defining a range where objects are ignored">00.000000,-3000.000000</exclusionRectP1>
        <exclusionRectP2
            class="edu.dhbw.mannheim.tigers.sumatra.model.data.shapes.vector.IVector2" comment="P2 of rectangle defining a range where objects are ignored">0.000000,3000.000000</exclusionRectP2>
    </SyncedCamFrameBufferV2>
    <kalman>
        <oextkal>
            <KalmanWorldFramePacker>
                <useBotStatus class="boolean" comment="override WP data with status from bot? (pos,vel)">true</useBotStatus>
            </KalmanWorldFramePacker>
            <data>
                <PredictionContext>
                    <stepSize class="double" comment="time which one lookahead prediction step should look ahead (in internalTime)">0.01</stepSize>
                    <stepCount class="int" comment="number of lookahead steps which should be performed">2</stepCount>
                    <numberParticle class="int" comment="number of particles used in particle filter">500</numberParticle>
                    <pfESS class="double" comment="threshold for particle filter when to resample">0.2</pfESS>
                    <resampler class="java.lang.String" comment="">stratified</resampler>
                    <filterTimeOffset class="double" comment="constant offset between capture time and now">0.001</filterTimeOffset>
                </PredictionContext>
            </data>
            <BallCorrector>
                <correctFlyingBalls class="boolean" comment="correct flying balls">false</correctFlyingBalls>
            </BallCorrector>
        </oextkal>
    </kalman>
</wp>
