<?xml version="1.0" encoding="UTF-8"?>
<edu.tigers.sumatra.skillsystem.skills base="edu.tigers.sumatra.skillsystem.skills">
    <AMoveSkill>
        <stopSpeedTolerance>0.4</stopSpeedTolerance>
    </AMoveSkill>
    <ApproachAndStopBallSkill>
        <catchUpBallTargetVelDiff>0.6</catchUpBallTargetVelDiff>
        <maxLookahead>0.6</maxLookahead>
    </ApproachAndStopBallSkill>
    <CatchBallSkill>
        <isOverAccelerationActive>false</isOverAccelerationActive>
        <keeperBlockPreventionMargin>300.0</keeperBlockPreventionMargin>
        <maxAcc>3.0</maxAcc>
        <maxChipInterceptDist>500.0</maxChipInterceptDist>
    </CatchBallSkill>
    <PenaltyShootSkill>
        <approachSpeed>0.15</approachSpeed>
        <dribbleSpeed>8000.0</dribbleSpeed>
        <slowMoveDist>200.0</slowMoveDist>
        <targetAngle>14.0</targetAngle>
    </PenaltyShootSkill>
    <SingleTouchKickSkill>
        <roughAngleTolerance>0.3</roughAngleTolerance>
    </SingleTouchKickSkill>
    <test>
        <LatencyTestSkill>
            <maxMeasures>5</maxMeasures>
            <maxSamples>100</maxSamples>
        </LatencyTestSkill>
    </test>
    <TouchKickSkill>
        <roughAngleTolerance>0.15</roughAngleTolerance>
    </TouchKickSkill>
    <util>
        <AroundBallCalc>
            <shiftAngle>0.7</shiftAngle>
        </AroundBallCalc>
        <BallInterceptor>
            <closestTimeThreshold>0.8</closestTimeThreshold>
            <maxAcc>6.0</maxAcc>
            <minAcc>2.3</minAcc>
        </BallInterceptor>
        <CatchBallCalc>
            <minBallVel>0.7</minBallVel>
        </CatchBallCalc>
    </util>
</edu.tigers.sumatra.skillsystem.skills>
