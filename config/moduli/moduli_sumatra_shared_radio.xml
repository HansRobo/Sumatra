		<implementation>edu.tigers.sumatra.wp.WorldInfoCollector</implementation>

		<properties>
		</properties>

		<dependency>cam</dependency>
		<dependency>timer</dependency>
	</module>


	<module id="referee" type="AReferee">
		<implementation> edu.tigers.sumatra.referee.Referee
		</implementation>

		<properties>
		</properties>
